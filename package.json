{
  "name": "@lexicon-labs/chat",
  "author": "lexicon-labs",
  "version": "1.0.0",
  "description": "Chat Widget Application",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": "~9.8.2",
    "yarn": "~1.22.22"
  },
  "scripts": {
    "dev": "vite --port 8082",
    "start": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "dev:host": "vite --host",
    "build:do": "yarn install --production=false && yarn run build && rm -rf node_modules && yarn install --production --frozen-lockfile"
  },
  "dependencies": {
    "@p5-wrapper/next": "^1.0.3",
    "@p5-wrapper/react": "^4.4.1",
    "@react-three/drei": "^9.106.0",
    "@react-three/fiber": "^8.16.8",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.16",
    "highlight.run": "^8.12.3",
    "history": "^5.3.0",
    "leva": "^0.9.35",
    "lodash": "^4.17.21",
    "micromark": "^4.0.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "three": "^0.165.0",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.12.1",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "less": "^4.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.6.2"
  }
}